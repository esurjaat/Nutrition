get_allrecipes("https://www.allrecipes.com/recipe/240400/skillet-chicken-bulgogi/")
get_allrecipes("https://www.allrecipes.com/recipe/246423/power-oatmeal/")

urls <- 
  c("https://www.allrecipes.com/recipe/267799/sauteed-mushroom-scrambled-eggs/",
    "https://www.allrecipes.com/recipe/240400/skillet-chicken-bulgogi/",
    "https://www.allrecipes.com/recipe/246423/power-oatmeal/",
    "https://www.allrecipes.com/recipe/72068/chicken-katsu/",
    "https://www.allrecipes.com/recipe/270771/sous-vide-salmon/")

temp <- 
  map_df(.x = urls,
       .f = ~get_allrecipes(.x) %>% 
         mutate(url = .x))
temp %>% View()